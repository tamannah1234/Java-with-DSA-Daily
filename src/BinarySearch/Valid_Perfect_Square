class Solution {
    public boolean isPerfectSquare(int num) {
    if(num==0 || num==1) return true;
    long low = 0; long high = num;
    boolean isPerfectSquare = false;   
    while(low<=high){
        long mid = low+(high-low)/2;
        long sq = mid*mid;
        if(sq==num) return true;
        else if(sq<num){
            low = mid+1;
        }else{
            high = mid-1;
        }
    }
    return isPerfectSquare; 
    }
}
